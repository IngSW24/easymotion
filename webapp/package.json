{
  "name": "webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --clearScreen false",
    "build": "npm run generate-client && tsc -b && vite build",
    "lint": "eslint .",
    "generate-client": "swagger-typescript-api -p /workspace/webapp/api-schema/schema.json -o ./src/client",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "@mui/x-date-pickers": "^7.24.0",
    "@tanstack/react-query": "^5.63.0",
    "framer-motion": "^12.0.0",
    "luxon": "^3.5.0",
    "marked": "^15.0.6",
    "notistack": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-phone-input-2": "^2.15.1",
    "react-router": "^7.1.1",
    "swagger-typescript-api": "^13.0.23"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@tanstack/react-query-devtools": "^5.63.0",
    "@testing-library/react": "^16.1.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.19.1",
    "vite": "^6.0.9",
    "vitest": "^2.1.8"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "proseWrap": "preserve",
    "overrides": [
      {
        "files": [
          "*.json",
          "*.yml",
          "*.yaml"
        ],
        "options": {
          "printWidth": 120,
          "tabWidth": 2
        }
      }
    ]
  }
}
