          },
          "phoneNumber": {
            "type": "string",
            "description": "Phone number of the new user (optional)",
            "example": "+123456789"
          },
          "birthDate": {
            "type": "string",
            "description": "Birth date in ISO format (YYYY-MM-DD) (optional)",
            "example": "1990-01-01"
          },
          "role": {
            "type": "string",
